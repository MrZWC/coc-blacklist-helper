<resources>
    <declare-styleable name="SearchEditText">
        <attr name="se_hint" format="string" />
    </declare-styleable>
    <declare-styleable name="YnLoadingLayout">
        <attr name="ynEmptyText" format="string" />
        <attr name="ynEmptyResId" format="reference" />
        <attr name="ynLoadingResId" format="reference" />
        <attr name="ynErrorResId" format="reference" />
        <!--是否当content有数据时不在显示错误页和空页面 默认 false-->
        <attr name="ynShowOnly" format="boolean" />
        <attr name="onClickRetry" format="reference" />
        <attr name="ynShowType">
            <enum name="content" value="0" />
            <enum name="error" value="1" />
            <enum name="loading" value="2" />
            <enum name="empty" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="CommonTitleToolBar">
        <attr name="ctb_title" format="string" />
    </declare-styleable>
    <declare-styleable name="AspectRatioFrameLayout">
        <attr name="aspectRatio" format="float" />
    </declare-styleable>
    <declare-styleable name="ClipLayout">
        <attr name="cl_roundSize" format="dimension|reference" />
        <attr name="cl_roundAsCircle" format="boolean" />
        <attr name="cl_showBorder" format="boolean" />
        <attr name="cl_borderSize" format="dimension|reference" />
        <attr name="cl_borderColor" format="color|reference" />
    </declare-styleable>
</resources>